{
  "plugins": [
    "jsdoc-plugin-typescript"
  ],
  "typescript": {
    "moduleRoot": "./app/assets/javascripts/vhl"
  },
  "recurseDepth": 10,
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_",
      "include": ["./app/assets/javascripts/vhl/"]
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./public/jsdoc/",
    "recurse": true
  }
}